{
	"sources": {
		"mygravity": {
			"subscriber_id": "native_adapter",
			"subscriber_name": "Native Adapter",
			"host": "0.0.0.0",
			"port": 32803,
			"ping_interval": 10,
			"max_pings_outstanding": 3,
			"max_reconnects": -1,
			"worker_count": 4,
			"chunk_size": 2048,
			"verbose": true,
			"initial_load": true,
			"omitted_count": 100000,
			"collections": [
				"users"
			],
			"events": {
				"snapshot": "accountInitialized",
				"create": "accountCreated",
				"update": "accountUpdated",
				"delete": "accountDeleted"
			}
		}
	}
}
